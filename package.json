{
  "name": "cow-list",
  "version": "0.0.0",
  "description": "Copy-on-write iterable list",
  "keywords": [
    "cow",
    "list",
    "rope",
    "search",
    "tree",
    "versioning"
  ],
  "author": "Victorien Elvinger (victorien.elvinger.fr)",
  "license": "Zlib",
  "repository": {
    "type": "git",
    "url": "https://github.com/Conaclos/cow-list"
  },
  "bugs": {
    "url": "https://github.com/Conaclos/cow-list/issues"
  },
  "type": "commonjs",
  "source": "src/index.ts",
  "main": "dist/main/index.js",
  "exports": {
    "require": "./dist/main/index.js",
    "default": "./dist/module/index.js"
  },
  "module": "dist/module/index.js",
  "es2015": "dist/es2015/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc -b tsconfig.main.json tsconfig.module.json tsconfig.types.json",
    "clean": "rm -rf coverage dist .tested",
    "pretest": "tsc",
    "test": "c8 ava"
  },
  "ava": {
    "files": [
      "tests/**/*.test.ts",
      "specs/**/*.test.ts"
    ],
    "typescript": {
      "rewritePaths": {
        "tests/": ".tested/tests/",
        "specs/": ".tested/specs/"
      }
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run clean && npm test && npm run build"
    }
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/node": "^14.10.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "ava": "^3.12.1",
    "ava-fast-check": "^3.0.0",
    "c8": "^7.3.0",
    "eslint": "^7.8.1",
    "eslint-config-conaclos": "^3.0.0",
    "fast-check": "^2.3.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "prettier": "~2.1.1",
    "typescript": "~4.0.2"
  },
  "dependencies": {
    "tslib": "^2.0.1"
  }
}
